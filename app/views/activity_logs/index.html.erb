<ul>
  <% @activity_log.each do |activity_log| %>
    <div class="card">
      <div class="card-header">
        <%= activity_log.created_at.to_s(:datetime_jp) %>
      </div>
      <div class="card-body">
        <a href="#"><%= activity_log.log %></a><br />
        <a href="#"><%= link_to '編集', edit_activity_log_path(@activity_log.ids) %></a>
      </div>
    </div>
  <% end %>
  <%= paginate @activity_log %>
</ul>
        
