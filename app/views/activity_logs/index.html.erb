<div class="box">
  <%= search_form_for @q do |f| %>
    キーワード検索<%= f.search_field :log_cont, class: "form-control" %><br />
    日付検索<%= f.date_field :created_at_gteq, class: "form-control" %>から<%= f.date_field :created_at_lteq, class: "form-control" %><br />
    <%= f.submit "検索" %>
  <% end %>
</div>
<% @activity_logs.each do |activity_log| %>
  <div class="card">
    <div class="card-header">
      <%= activity_log.created_at.to_s(:datetime_jp) %>
    </div>
    <div class="card-body">
      <%= simple_format(activity_log.log) %><br />
      <%= link_to '編集', edit_activity_log_path(activity_log.id) %>
    </div>
  </div>
<% end %>
<%= paginate @activity_logs %>
        
